<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ab.umphoto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".test.SaveImage"/>
        <activity android:name=".test.GetImage"/>
        <activity android:name=".test.ZoomImage"/>
        <activity android:name=".test.GrayImage"/>
        <activity android:name=".test.MergeImage"/>
        <activity android:name=".test.MixImage"/>
        <activity android:name=".test.ReflectionImage"/>
        <activity android:name=".test.RoundCornerImage"/>

    </application>

</manifest>